class Solution(object):
    def isCovered(self, ranges, left, right):
        """
        :type ranges: List[List[int]]
        :type left: int
        :type right: int
        :rtype: bool
        """
        for i in range(left, right+1):
            for r in ranges:
                for j in range (2):
                    if i>= r[j] and i<=r[j]:
                        return True
        return False
